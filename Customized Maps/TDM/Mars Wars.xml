<?xml version="1.0"?>
<map proto="1.3.6">
<include src="blitz-global.xml"/>
<name>Mars Wars</name>
<version>1.0.0</version>
<objective>Eliminate the others teams before 8 minutes is up</objective>
<authors>
<author contribution="Map and XML">Everdeen_</author>
</authors>
<teams>
<team color="dark red" max="15" max-overfill="17">Mars</team>
<team color="gray" max="15" max-overfill="17">Deimos</team>
<team color="dark aqua" max="15" max-overfill="17">Fobos</team>
</teams>
<spawns>
<spawn team="Mars" kit="Marte">
<regions yaw="180">
<block>-1151,27,-492</block>
</regions>
</spawn>
<spawn team="Deimos" kit="Deim">
<regions yaw="45">
<block>-1121,27,-558</block>
</regions>
</spawn>
<spawn team="Fobos" kit="IMAH0B0">
<regions yaw="-50">
<block>-1184,27,-558</block>
</regions>
</spawn>
<default>
<regions yaw="90">
<block>-1149,89,-599</block>
</regions>
</default>
</spawns>
<kits>
<kit name="spawn" force="true">
<item slot="103">glass</item>
<item slot="0" enchantment="damage all:100">diamond sword</item>
<item slot="1" enchantment="arrow damage:100">bow</item>
<item slot="2" amount="8">golden apple</item>
<item slot="3" amount="1">arrow</item>
<potion duration="5" amplifier="30">heal</potion>
<potion duratiom="oo" amplifier="1">jump</potion>
</kit>
<kit name="Marte" parents="spawn">
<chestplate color="B85933">leather chestplate</chestplate>
<boots enchantment="protection fall:3">diamond boots</boots>
</kit>
<kit name="Deim" parents="spawn">
<chestplate color="999999">leather chestplate</chestplate>
<boots enchantment="protection fall:3">diamond boots</boots>
</kit>
<kit name="IMAH0B0" parents="spawn">
<chestplate color="6699D8">leather chestplate</chestplate>
<boots enchantment="protection fall:3">diamond boots</boots>
</kit>
</kits>
<regions>
<apply block="deny-blocks">
<rectangle name="map" min="oo,oo" max="-oo,-oo"/>
</apply>
</regions>
<blitz>
<time>8m</time>
<lives>2</lives>
<title>Blitz: Rage</title>
</blitz>
<itemremove>
<item>diamond sword</item>
<item>bow</item>
<item>golden apple</item>
<item>leather chestplate</item>
<item>diamond boots</item>
</itemremove>
<killreward>
<item>arrow</item>
</killreward>
</map>
